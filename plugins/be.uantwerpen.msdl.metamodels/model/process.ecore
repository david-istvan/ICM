<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="process" nsURI="metamodels.process" nsPrefix="be.uantwerpen.msdl.metamodels">
  <eClassifiers xsi:type="ecore:EClass" name="NamedElement" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ProcessModel" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="process" upperBound="-1"
        eType="#//Process" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="transformationGraph" eType="#//TransformationGraph"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="propertyModel" eType="#//PropertyModel"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="costModel" eType="#//CostModel"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="intent" upperBound="-1"
        eType="#//Intent" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Process" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="node" upperBound="-1" eType="#//Node"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="controlFlow" upperBound="-1"
        eType="#//ControlFlow" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ControlFlow" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="toNode" upperBound="-1"
        eType="#//Node"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="fromNode" upperBound="-1"
        eType="#//Node"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="final" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"
        defaultValueLiteral="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Node" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EReference" name="dataFlowFrom" upperBound="-1"
        eType="#//Node" eOpposite="#//Node/dataFlowTo"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="dataFlowTo" upperBound="-1"
        eType="#//Node" eOpposite="#//Node/dataFlowFrom"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Activity" abstract="true" eSuperTypes="#//CostItem #//NamedElement #//Node">
    <eStructuralFeatures xsi:type="ecore:EReference" name="typedBy" eType="#//Transformation"
        eOpposite="#//Transformation/types"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ManualActivity" eSuperTypes="#//Activity"/>
  <eClassifiers xsi:type="ecore:EClass" name="AutomatedActivity" eSuperTypes="#//Activity"/>
  <eClassifiers xsi:type="ecore:EClass" name="Object" eSuperTypes="#//NamedElement #//Node">
    <eStructuralFeatures xsi:type="ecore:EReference" name="typedBy" lowerBound="1"
        eType="#//Language" eOpposite="#//Language/types"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Control" abstract="true" eSuperTypes="#//Node"/>
  <eClassifiers xsi:type="ecore:EClass" name="Initial" eSuperTypes="#//Control"/>
  <eClassifiers xsi:type="ecore:EClass" name="Final" abstract="true" eSuperTypes="#//Control"/>
  <eClassifiers xsi:type="ecore:EClass" name="ActivityFinal" eSuperTypes="#//Final"/>
  <eClassifiers xsi:type="ecore:EClass" name="FlowFinal" eSuperTypes="#//Final"/>
  <eClassifiers xsi:type="ecore:EClass" name="Fork" eSuperTypes="#//Control"/>
  <eClassifiers xsi:type="ecore:EClass" name="Join" eSuperTypes="#//Control"/>
  <eClassifiers xsi:type="ecore:EClass" name="Merge" eSuperTypes="#//Control"/>
  <eClassifiers xsi:type="ecore:EClass" name="Decision" eSuperTypes="#//Control #//NamedElement"/>
  <eClassifiers xsi:type="ecore:EClass" name="TransformationGraph">
    <eStructuralFeatures xsi:type="ecore:EReference" name="transformation" upperBound="-1"
        eType="#//Transformation" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="language" upperBound="-1"
        eType="#//Language" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Transformation" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="inputLanguage" upperBound="-1"
        eType="#//Language" eOpposite="#//Language/inputOf"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="outputLanguage" upperBound="-1"
        eType="#//Language" eOpposite="#//Language/outputOf"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="types" upperBound="-1"
        eType="#//Activity" eOpposite="#//Activity/typedBy"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Language" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="inputOf" upperBound="-1"
        eType="#//Transformation" eOpposite="#//Transformation/inputLanguage"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="outputOf" upperBound="-1"
        eType="#//Transformation" eOpposite="#//Transformation/outputLanguage"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="types" upperBound="-1"
        eType="#//Object" eOpposite="#//Object/typedBy"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CostModel">
    <eStructuralFeatures xsi:type="ecore:EReference" name="cost" upperBound="-1" eType="#//Cost"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CostItem" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EReference" name="cost" eType="#//Cost" eOpposite="#//Cost/costItem"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Cost" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EReference" name="costItem" lowerBound="1"
        eType="#//CostItem" eOpposite="#//CostItem/cost"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="RatioScale" eSuperTypes="#//Cost">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Ordinal" abstract="true" eSuperTypes="#//Cost"/>
  <eClassifiers xsi:type="ecore:EClass" name="Expensive" eSuperTypes="#//Ordinal"/>
  <eClassifiers xsi:type="ecore:EClass" name="Cheap" eSuperTypes="#//Ordinal"/>
  <eClassifiers xsi:type="ecore:EClass" name="PropertyModel">
    <eStructuralFeatures xsi:type="ecore:EReference" name="property" upperBound="-1"
        eType="#//Property" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="constraint" upperBound="-1"
        eType="#//AbstractConstraint" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Property" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="ontologicalRelationship"
        upperBound="-1" eType="#//OntologicalRelationship" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="constraintOnProperty" upperBound="-1"
        eType="#//AbstractConstraint" eOpposite="#//AbstractConstraint/constrainedProperty"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="intentOfProperty" upperBound="-1"
        eType="#//Intent" eOpposite="#//Intent/propertyOfIntent"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OntologicalRelationship" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="ontologicalRelationshipTo"
        eType="#//Property"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AbstractConstraint" abstract="true"
      eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="constrainedProperty" upperBound="-1"
        eType="#//Property" eOpposite="#//Property/constraintOnProperty"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="hard" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"
        defaultValueLiteral="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="quality" lowerBound="1"
        eType="#//ExpressionQuality" defaultValueLiteral="L1"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Requirement" eSuperTypes="#//AbstractConstraint"/>
  <eClassifiers xsi:type="ecore:EClass" name="Constraint" eSuperTypes="#//AbstractConstraint"/>
  <eClassifiers xsi:type="ecore:EEnum" name="ExpressionQuality">
    <eLiterals name="L1"/>
    <eLiterals name="L2" value="1"/>
    <eLiterals name="L3" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Expression" abstract="true"/>
  <eClassifiers xsi:type="ecore:EClass" name="L1" eSuperTypes="#//Expression"/>
  <eClassifiers xsi:type="ecore:EClass" name="L2" eSuperTypes="#//Expression"/>
  <eClassifiers xsi:type="ecore:EClass" name="L3" eSuperTypes="#//Expression"/>
  <eClassifiers xsi:type="ecore:EClass" name="Infinite" eSuperTypes="#//RatioScale"/>
  <eClassifiers xsi:type="ecore:EClass" name="Intent">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" eType="#//IntentType"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="propertyOfIntent" upperBound="-1"
        eType="#//Property" eOpposite="#//Property/intentOfProperty"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="activity" lowerBound="1"
        eType="#//Activity"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="IntentType">
    <eLiterals name="UNKNOWN" literal="UNKNOWN"/>
    <eLiterals name="ADD" value="1"/>
    <eLiterals name="READ" value="2"/>
    <eLiterals name="MODIFY" value="3"/>
    <eLiterals name="CHECK" value="4"/>
    <eLiterals name="EVALUATE" value="5"/>
  </eClassifiers>
</ecore:EPackage>
